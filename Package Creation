package calculator;

public class DoubleCalculator {
    public double add(double a, double b) {
        return a + b;
    }

    public double sub(double a, double b) {
        return a - b;
    }

    public double mul(double a, double b) {
        return a * b;
    }

    public double div(double a, double b) {
        return a / b;
    }
}

//its an another file where the package is importing from 1st file package named calculator.
import calculator.DoubleCalculator;

public class Calc {
    public static void main(String[] args) {
        DoubleCalculator c1 = new DoubleCalculator();
        double r;

        r = c1.add(10.0, 20.0);
        System.out.println("sum = " + r);

        r = c1.sub(10.0, 20.0);
        System.out.println("diff = " + r);

        r = c1.mul(10.0, 20.0);
        System.out.println("product = " + r);

        r = c1.div(10.0, 20.0);
        System.out.println("div = " + r);
    }
}

